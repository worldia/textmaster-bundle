{
    "name": "worldia/textmaster-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 bundle integrating php-textmaster-api",
    "homepage": "https://github.com/worldia/textmaster-bundle",
    "keywords": ["textmaster", "translation", "api", "bundle"],
    "license": "UNLICENSED",
    "authors": [
        {
            "name": "Pierre Ducoudray",
            "email": "pducoudray@worldia.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/framework-bundle": "^2.7",
        "worldia/textmaster-api": "^0.2",
        "symfony/property-access": "^2.7",
        "symfony/validator": "^2.7",
        "symfony/form": "^2.7",
        "doctrine/orm": "^2.3",
        "doctrine/doctrine-bundle": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "matthiasnoback/symfony-config-test": "^1.4",
        "matthiasnoback/symfony-dependency-injection-test": "0.*",
        "behat/symfony2-extension": "2.0.*"
    },
    "suggest": {
        "white-october/pagerfanta-bundle": "Needed to display pager on list template if you add routing."
    },
    "autoload": {
        "psr-4": { "Worldia\\Bundle\\TextmasterBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
